*{
    padding: 0px;
    margin: 0px;
    font-family: "Microsoft YaHei UI";
}
.container{
    background: url("../images/b413f1c448f99f5c1b1e8bd360ae6c15.jpg");
    background-size: 100% 100%;
    overflow: hidden;
}
.myAccount_login,.mySms_login{
    width: 30%;
    margin:auto;
    overflow: hidden;
    text-align: center;
}
.container h1{
    color: rgba(8, 156, 36, 0.69);
    font-size: 48px;
    text-align: center;
    margin-top: 100px;
}
.myAccount_login input:nth-of-type(1),
.myAccount_login input:nth-of-type(2),
.mySms_login input:nth-of-type(1),
.mySms_login input:nth-of-type(2){
    outline: none;
    width: 70%;
    height: 40px;
    margin: 10px auto;
    text-indent: 10px;
    border: 1px #c6c6c6 solid;
    color: rgb(114, 113, 113);
}
.mySms_login input:nth-of-type(2){
    width: 50%;
    float: left;
    margin-left: 15%;
}
.myAccount_login input:nth-of-type(3){
    float: left;
    margin-left: 15%;
}
.myAccount_login span{
    float: left;
    margin-left: 1%;
    font-size: 12px;
    color: rgb(114, 113, 113);
}
.myinput button{
    outline: none;
    width: 70%;
    height: 40px;
    margin: 10px auto 30px auto;
    border: 1px #c6c6c6 solid;
    background-color: #0ac62e;
    color: white;
    font-size: 16px;
}
.myinput:after{
     content: '';
     clear: both;
     height: 0px;
     display: none;
     visibility: hidden;
 }
.myinput span:nth-of-type(2){
    float: right;
    margin-right: 15%;
    color: rgb(114, 113, 113);
    text-decoration: none;
    font-size: 12px;
    cursor: pointer;
}

.myinput{
    text-align: center;
    width: 90%;
    border: 1px rgba(78, 230, 64, 0.6) solid;
    margin: 30px auto;
    border-radius: 5px;
    background-color: rgba(110, 255, 124, 0.4);
}
.myinput p{
    font-size: 12px;
    color: rgb(114, 113, 113);
    float: right;
    margin-right: 15%;
    margin-top: 20px;
    cursor: pointer;
}
.mySms_login,.myregister{
    display: none;
}
.mySms_login span{
    color: rgb(114, 113, 113);
    font-size: 12px;
    background-color: #299ac6;
    width: 20%;
    height: 42px;
    display: inline-block;
    margin-top: 10px;
    margin-left: -15%;
    color: white;
    text-align: center;
    line-height: 40px;
    cursor: pointer;
}
.myregister{
    width:30%;
    margin: auto;
}
.myregister input[type="text"],.myregister input[type="password"]{
    outline: none;
    width: 90%;
    height: 34px;
    margin: 4px auto;
    text-indent: 10px;
    border: 1px #c6c6c6 solid;
    color: rgb(114, 113, 113);
}
.myregister tr:nth-of-type(1) input{
    margin-top: 20px;
}
.myregister tr:nth-last-of-type(1) button,
.myregister tr:nth-last-of-type(1) input{
    margin-bottom: 20px;
}
.myregister button{
    width: 60%;
    margin: 0px;
}
.myregister label{
    font-size: 14px;
    color: rgb(114, 113, 113);
}
#mytable{
    margin-left: 5%;
    width:90%;
}
#mytable td{
    width: 50%;
    margin: auto;
    height: 45px;
}
#mytable span{
    color: rgb(114, 113, 113);
    font-size: 12px;
    background-color: #299ac6;
    border-top: 1px #299ac6 solid;
    border-bottom: 1px #299ac6 solid;
    width: 20%;
    height: 34px;
    display: inline-block;
    color: white;
    text-align: center;
    line-height: 36px;
    cursor: pointer;
}
#verification{
    width: 70%;
}
#emailTestTd,
#phoneTestTd{
    position: relative;
}
#emailTestBox,
#phoneTestBox{
    position: absolute;
    top: -5px;
    left: 85%;
    width: 16px;
    height: 16px;
    overflow: hidden;
}
.icon-zhengque{
    color: #0ac62e;
}
.icon-cuowuguanbi-{
    color: red;
}