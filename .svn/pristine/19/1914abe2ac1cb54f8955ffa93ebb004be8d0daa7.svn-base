section{
    background-color: #f5f5f5;
}
h2{
    margin:10px 0 20px;
}
.myShippingAddress{
    width:100%;
    background-color: #fff;
    padding:20px 0 0;
}

.myShippingAddressHead{
    padding:0 3% 10px;
    border-bottom: 1px #b0a296 solid;
}
.addAddress{
    width:18%;
    height:25px;
    background-color: #b0a296;
    color: #fff;
    text-align: center;
    line-height: 25px;
    cursor: pointer;
}
.addressTable{
    padding-top:20px;
}
.addressTableContent{
    padding: 0;
}
.nulladdress>p{
    padding:10px 0 330px;
}
.detailedAddress>p,
.recipientName>p,
.recipientPhone>p,
.fixedPhone>p{
    padding:10px 0 5px;
}
.detailedAddress>div,
.recipientPhone>div,
.fixedPhone>div{
    width:60%;
    height:25px;
    border:1px solid #b0a296;
}
.detailedAddress>div>input,
.recipientName>div>div>input,
.recipientPhone>div>input,
.fixedPhone>div>input{
    width:100%;
    outline: none;
    border:none;
    padding:0 5px;
}
.recipientName,
.defaultAdress{
    padding:10px 0 5px;
}
.recipientName>div{
    width:50%;
}
.recipientName>div>div{
    width:70%;
    height:25px;
    border:1px solid #b0a296;
}

.modAddress,
.delAddress{
    width:60%;
    height:25px;
    text-align: center;
    border-radius: 5px;
    color:#fff;
    cursor: pointer;
    margin-bottom:5px;
    margin-left:8%;
    line-height: 25px;
}
.modAddress{
    background-color: rgb(3, 165, 3);
}
.delAddress{
    background-color: rgb(185, 41, 41);
}
.defAddress{
    width:80%;
    height:25px;
    text-align: center;
    border-radius: 5px;
    color:#fff;
    cursor: pointer;
    margin-bottom:5px;
    line-height: 25px;
}
.defAddress{
    background-color: rgb(177, 185, 54);
}
