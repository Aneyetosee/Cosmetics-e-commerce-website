section{
    background-color: #f5f5f5;
}
h2{
    margin:10px 0 20px;
}

.myOrder{
    width:100%;
    background-color: #fff;
    padding:20px 0 0;
}
.myOrderContent{
    padding:0 20px;
}
th{
    text-align: center;
}
.backIndex{
    width:18%;
    height:30px;
    background-color: #b0a296;
    color: #fff;
    text-align: center;
    line-height: 30px;
    cursor: pointer;
}
.nullOrder>div:first-child,
.nullOrder>div:nth-child(2),
.nullOrder>div:nth-child(3),
.nullOrder>div:nth-child(4){
    background-color: #b0a296;
    height:25px;
    line-height:25px;
}

.nullOrder>p{
    padding:10px 0;
}
.nullOrder>div{
    margin:10px 0 25px;
}

.orderTableContent{
    padding: 0;
}