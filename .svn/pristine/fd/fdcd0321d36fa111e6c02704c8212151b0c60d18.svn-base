/* section{
    background-color: #f5f5f5;
} */
h2{
    margin:10px 0 20px;
}

.myOrder{
    width:100%;
    background-color: #fff;
    padding:20px 0 0;
}
.myOrderContent{
    padding:0 20px;
}
th{
    text-align: center;
}
.backIndex{
    width:18%;
    height:30px;
    background-color: #b0a296;
    text-align: center;
    line-height: 30px;
    cursor: pointer;
}
.backIndex>a{
    color: #fff;
}

.orderTableContent{
    padding: 0;
}
.searchOrderBox{
    border:1px solid #000;
}
#searchOrder{
    outline: none;
    height: 24px;
    text-indent: 1em;
    border:none;
}
#searchOrderBtn{
    outline: none;
    border:none;
    text-align: center;
    background-color: transparent;
}
.unpaid{
    text-decoration:underline rgb(133, 7, 7);
    color:rgb(133, 7, 7);
    cursor: pointer;
}
.makeSure{
    width:80%;
    height:25px;
    line-height:25px;
    margin:0 auto;
    background-color: #b0a296;
    color:#fff;
    cursor: pointer;
}
.orderDetails{
    text-decoration:underline rgb(31, 18, 212);
    color:rgb(31, 18, 212);
    cursor: pointer;
    margin:5px 0;
}
.clearRoder{
    text-decoration:underline rgb(185, 92, 29);
    color:rgb(185, 92, 29);
    cursor: pointer;
    margin:5px 0;
}
/* 订单表格 */
.orderTableTr{
    text-align: center;
}
.orderNumber{
    width:10%;
}
.shipContent{
    width:35%;
}
.shipContent>img{
    width:30%;
}
.commentsOnTheGoods{
    cursor: pointer;
}